syntax on
colorscheme Papercolor
" colorscheme onedark
set background=dark
set lines=30 columns=100
set guioptions-=m
set guioptions-=T
set guioptions-=l
set guioptions-=r
set visualbell
set vb t_vb=
set laststatus=2
set backspace=2

language en_US

" set guioptions+=r
" au VimEnter * if line('$') > &lines | set go+=r | else | set go-=r | endif
" au VimResized * if line('$') > &lines | set go+=r | else | set go-=r | endif
