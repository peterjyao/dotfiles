{
    "notebook.lineNumbers": "on",
    "update.enableWindowsBackgroundUpdates": false,
    "update.showReleaseNotes": false,
    "update.mode": "none",
    "editor.rulers": [80

    ],
    "quarto.mathjax.theme": "dark",
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "vim.useSystemClipboard": true,
    "terminal.integrated.defaultProfile.windows": "Command Prompt",
    "breadcrumbs.enabled": false,
    "editor.occurrencesHighlight": true,
    "jupyter.experiments.enabled": false,
    "editor.parameterHints.enabled": false,
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnType": true
    },
    "csv-edit.fontFamilyInTable": "sameAsCodeEditor",
    "csv-edit.fontSizeInPx": 0,
    "csv-edit.readOption_hasHeader": "true",
    "csv-edit.initiallyIsInReadonlyMode": true,
    "security.workspace.trust.untrustedFiles": "open",
    "editor.minimap.enabled": false,
    "jupyter.generateSVGPlots": true,
    "gitlens.currentLine.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.codeLens.enabled": false,
    "remote.SSH.remotePlatform": {
        "10.0.25.11": "linux",
        "mmpdev": "linux"
    },
    "window.restoreWindows": "none",
    "jupyter.askForKernelRestart": false,
    "activeDocumentIndicator.opacity": 30,
    "workbench.editor.closeEmptyGroups": false,
    "workbench.editor.wrapTabs": true,
    "mssql.connections": [
        {
            "server": "CHVMV02",
            "database": "",
            "authenticationType": "Integrated",
            "password": "",
            "encrypt": "Mandatory",
            "trustServerCertificate": true
        }
    ],
    "vscode-json-editor.theme": "dark",
    "dance.modes": {

        "": {
            "hiddenSelectionsIndicatorsDecoration": {
                "after": {
                    "color": "$list.warningForeground"
                },
                "backgroundColor": "$inputValidation.warningBackground",
                "borderColor": "$inputValidation.warningBorder",
                "borderStyle": "solid",
                "borderWidth": "1px",
                "isWholeLine": true
            }
        },
        "input": {
            "cursorStyle": "underline-thin"
        },
        "insert": {
            "onLeaveMode": [
                [
                    ".selections.save",
                    {
                        "register": " insert"
                    }
                ]
            ]
        },
        "visual": {
            "lineNumbers": "on",
            "cursorStyle": "underline",
            "selectionBehavior": "character",
            "onEnterMode": [
                [
                    ".selections.restore",
                    {
                        "register": " ^",
                        "try": true
                    }
                ]
            ],
            "onLeaveMode": [
                [
                    ".selections.save",
                    {
                        "register": " ^",
                        "style": {
                            "borderColor": "$editor.selectionBackground",
                            "borderStyle": "solid",
                            "borderWidth": "2px",
                            "borderRadius": "1px"
                        },
                        "until": [
                            [
                                "mode-did-change",
                                {
                                    "include": "normal"
                                }
                            ],
                            [
                                "mode-did-change",
                                {
                                    "include": "visual"
                                }
                            ],
                            [
                                "selections-did-change"
                            ]
                        ]
                    }
                ]
            ]
        },
        "normal": {
            "lineNumbers": "on",
            "cursorStyle": "block",
            "selectionBehavior": "character",
            "onEnterMode": [
                [
                    ".selections.restore",
                    {
                        "register": " ^",
                        "try": true
                    }
                ]
            ],
            "onLeaveMode": [
                [
                    ".selections.save",
                    {
                        "register": " ^",
                        "style": {
                            "borderColor": "$editor.selectionBackground",
                            "borderStyle": "solid",
                            "borderWidth": "2px",
                            "borderRadius": "1px"
                        },
                        "until": [
                            [
                                "mode-did-change",
                                {
                                    "include": "normal"
                                }
                            ],
                            [
                                "mode-did-change",
                                {
                                    "include": "visual"
                                }
                            ],
                            [
                                "selections-did-change"
                            ]
                        ]
                    }
                ]
            ]
        }
    },
    "terminal.integrated.fontWeightBold": "normal",
    "jupyter.interactiveWindow.creationMode": "perFile",
    "jupyter.interactiveWindow.textEditor.autoMoveToNextCell": false,
    "dateTime.customFormat": "\tYYYY-MM-DD > [Week] W > dddd > HH:mm:ss\t",
    "workbench.colorTheme": "Sonokai",
    "dateTime.locale": "de-ch",
    "dateTime.timeZone": "Europe/Zurich",
    "dateTime.clipboardFormat": "",
    "git.confirmSync": false,
    "editor.fontWeight": "350",
    "workbench.editor.autoLockGroups": {
        "terminalEditor": false
    },
    "files.trimTrailingWhitespace": true,
    "[sql]": {
        "editor.defaultFormatter": "mtxr.sqltools"
    },
    "editor.fontFamily": " 'Cascadia Mono','Dank Mono', Consolas, 'Courier New', monospace",
    "editor.fontSize": 13
}
